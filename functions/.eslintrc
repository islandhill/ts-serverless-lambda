{
  "parser": "babel-eslint",
  "globals": {
    "$": false
  },
  "rules": {
    "no-unused-expressions": [0],
    "no-underscore-dangle": [0],
    "quote-props": [2, "as-needed", {"keywords": true}],
    "no-multi-spaces": [0],
    "no-unused-vars": [0],
    "no-loop-func": [0],
    "new-cap": [0],
    "key-spacing": [0],
    "strict": [0],
    "indent": [2, 2],
    "quotes": [
      1,
      "single",
      "avoid-escape"
    ],
    "curly": [0],
    "max-len": [1, 300]
  },
  "plugins": [],
  "settings": {
    "ecmascript": 6
  },
  "env": {
    "node": true,
    "jasmine": true
  }
}